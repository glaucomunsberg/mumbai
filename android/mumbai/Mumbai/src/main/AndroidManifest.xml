<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.paraformalidade.mumbai" >

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_paraformalidade_icon"
        android:label="@string/develop_name"
        android:theme="@style/Theme.Base.AppCompat.Light.DarkActionBar">
        <activity
            android:name="com.paraformalidade.mumbai.Mumbai"
            android:configChanges="orientation|keyboardHidden|screenSize"
            android:label="@string/develop_name"
            android:theme="@style/FullscreenTheme" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.paraformalidade.mumbai.menu"
            android:label="@string/activity_menu" >
        </activity>

    </application>

</manifest>
